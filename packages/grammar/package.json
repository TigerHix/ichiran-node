{
  "name": "@ichiran/grammar",
  "version": "0.0.0",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": "./dist/index.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.json && cp -r src/grammars dist/",
    "build:watch": "concurrently \"tsc -p tsconfig.json --watch\" \"chokidar 'src/grammars/**/*.json' -c 'cp -r src/grammars dist/' --initial\"",
    "typecheck": "tsc --noEmit",
    "test": "bun test tests/"
  },
  "dependencies": {
    "@ichiran/core": "0.0.0",
    "lru-cache": "^10.2.2"
  },
  "sideEffects": false
}

