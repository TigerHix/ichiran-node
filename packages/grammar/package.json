{
  "name": "@ichiran/grammar",
  "version": "0.0.0",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": "./dist/index.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.json && cp -r src/grammarMatcher/grammars dist/grammarMatcher/",
    "build:watch": "concurrently \"tsc -p tsconfig.json --watch\" \"chokidar 'src/grammarMatcher/grammars/**/*.json' -c 'cp -r src/grammarMatcher/grammars dist/grammarMatcher/' --initial\"",
    "typecheck": "tsc --noEmit",
    "test": "bun test tests/"
  },
  "dependencies": {
    "@ichiran/core": "0.0.0"
  },
  "sideEffects": false
}

